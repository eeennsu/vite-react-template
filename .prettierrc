{
    "printWidth": 130,
    "tabWidth": 4,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "singleAttributePerLine": true
}
